{
  "version": 2,
  "builds": [
      {
          "src": "./index.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      { 
          "src": "/api/products/(.*)", 
          "dest": "/api/products/$1" 
      }
          ,
      {
          "src": "/(.*)",
          "dest": "/"
      }
  ]
}