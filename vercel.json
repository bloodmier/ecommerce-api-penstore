{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/products/([a-zA-Z0-9]{24})",
        "dest": "/products/:id"
      },
      {
        "src": "/products/(.*)",
        "dest": "/products/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  